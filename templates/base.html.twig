<html>
 <head>
   <meta charset="UTF-8" />
   <title>
     {% block title %}
       Welcome!
     {% endblock %}
   </title>
   <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>" />
   <script src="https://code.jquery.com/jquery-3.6.1.min.js" integrity="sha256-o88AwQnZB+VDvE9tvIXrMQaPlFFSUTR+nldQm1LuPXQ=" crossorigin="anonymous"></script>
   <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />
 
   {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
   {% block stylesheets %}
     {{ encore_entry_link_tags('app') }}
   {% endblock %}
 
   {% block javascripts %}
     {{ encore_entry_script_tags('app') }}
   {% endblock %}
 </head>
 <body style="background-color: #171926;">
   <header class="navbar navbar-expand-lg navbar-dark bg-dark">
     <div class="container-fluid">
       <img src="data:image/png;base64," style="width: 220px;" data-ll-status="loaded" class="entered lazyloaded" width="253" height="28" />
       <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
       <div class="collapse navbar-collapse" id="navbarColor02">
         <ul class="navbar-nav me-auto mb-2 mb-lg-0">
           <li class="nav-item me-1 ms-5">
             <a class="btn btn-info btnGetNews" aria-current="page" href="#">Спецпроекты</a>
           </li>
         </ul>
       </div>
     </div>
   </header>
 
   <div id="alertPlaceholder"></div>
 
   <script type="text/javascript">
     var alertPlaceholder = document.getElementById('alertPlaceholder')
    
     function alert(message, type) {
       var wrapper = document.createElement('div')
       wrapper.innerHTML = '<div class="alert alert-' + type + ' alert-dismissible" role="alert">' + message + '<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button></div>'
       alertPlaceholder.append(wrapper)
     }
    
     $(document).on('click', '.btnGetNews', function (e) {
       $.get('/Home/GetContents', function (data) {
         console.log(data);
         //alert(data, 'success')
       })
     })
   </script>
 
   {% block body %}
 
   {% endblock %}
   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
 </body>
</html>
 
 

